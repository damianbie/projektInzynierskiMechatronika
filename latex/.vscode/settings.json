{
    "latex-workshop.latex.toolchain": [
        {
            "command": "latexdocker.cmd",
            "args": [
                "latexmk",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "%DOCFILE%"
            ]
        }
    ],
    "cSpell.language": "en,pl"
}